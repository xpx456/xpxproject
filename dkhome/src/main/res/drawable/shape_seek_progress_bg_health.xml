<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:startColor="#00f5f5f5"
                android:centerColor="#00f5f5f5"
                android:centerY="0.75"
                android:endColor="#00f5f5f5"
                android:angle="0"
                />
            <stroke
                android:color="#ff5e3a"
                android:width="1dp"
                />
            <corners
                android:radius="25dp"/>


        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#00f5f5f5"
                    android:centerColor="#00f5f5f5"
                    android:centerY="0.75"
                    android:endColor="#00f5f5f5"
                    android:angle="0"
                    />
                <stroke
                    android:color="#ff5e3a"
                    android:width="1dp"
                    />
                <corners
                    android:radius="25dp"/>
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#fcee0a"
                    android:centerColor="#fc930a"
                    android:centerY="0.75"
                    android:endColor="#ff5e3a"
                    android:angle="0"
                    />
                <stroke
                    android:color="#ff5e3a"
                    android:width="1dp"
                    />
                <corners
                    android:radius="25dp"/>
            </shape>
        </clip>
    </item>

</layer-list>

