<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg005">


    <androidx.viewpager.widget.ViewPager
        android:id="@+id/pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></androidx.viewpager.widget.ViewPager>

    <TextView
        android:id="@+id/time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/head"
        android:gravity="center_vertical|center_horizontal"
        android:textColor="#1296db"
        android:textSize="30dp" />

    <ImageView
        android:id="@+id/start"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:src="@drawable/start" />


    <ImageView
        android:id="@+id/map"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="10dp"
        android:layout_toRightOf="@+id/start"
        android:src="@drawable/map" />

    <ImageView
        android:id="@+id/des"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="10dp"
        android:layout_toRightOf="@+id/map"
        android:src="@drawable/des" />

    <ImageView
        android:id="@+id/watch"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:layout_toLeftOf="@+id/start"
        android:src="@drawable/watch" />

    <ImageView
        android:id="@+id/add"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="10dp"
        android:layout_toLeftOf="@+id/watch"
        android:src="@drawable/add" />


    <ImageView
        android:id="@+id/setting"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:src="@drawable/setting" />

    <ImageView
        android:id="@+id/tv"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="10dp"
        android:src="@drawable/tv" />


    <ImageView
        android:id="@+id/bluetooth"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_toLeftOf="@+id/setting"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="10dp"
        android:src="@drawable/lanya" />


    <ImageView
        android:id="@+id/left"
        android:layout_width="43dp"
        android:layout_height="90dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="5dp"
        android:src="@drawable/left" />


    <ImageView
        android:id="@+id/right"
        android:layout_width="43dp"
        android:layout_height="90dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="5dp"
        android:src="@drawable/right" />

    <intersky.mywidget.VerticalSeekBarWrapper
        android:layout_width="20dp"
        android:layout_height="80dp"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="10dp"
        android:layout_toRightOf="@+id/des"
        android:paddingLeft="0dp"
        android:paddingRight="0dp">

        <intersky.mywidget.VerticalSeekBar
            android:id="@+id/power"
            android:layout_width="20dp"
            android:layout_height="0dp"
            android:progressDrawable="@drawable/shape_seek_progress_bg2"
            android:splitTrack="false"
            android:paddingLeft="0dp"
            android:paddingRight="0dp"
            android:thumb="@drawable/shape_seek_thumb"
            app:seekBarRotation="CW270" />



    </intersky.mywidget.VerticalSeekBarWrapper>

    <RelativeLayout
        android:id="@+id/head"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/head_select_bg">

        <RelativeLayout
            android:id="@+id/headselect"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_alignParentTop="true">

            <intersky.mywidget.CircleImageView
                android:id="@+id/headicon"
                android:layout_width="19dp"
                android:layout_height="19dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="2.5dp"
                android:scaleType="centerCrop"
                android:src="@drawable/default_user" />

            <TextView
                android:id="@+id/headname"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_marginRight="2.5dp"
                android:layout_toRightOf="@+id/headicon"
                android:ellipsize="end"
                android:gravity="left|center_vertical"
                android:singleLine="true"
                android:text=""
                android:textColor="#000000"
                android:textSize="20dp" />

        </RelativeLayout>


        <ListView
            android:id="@+id/headlist"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_below="@+id/headselect"
            android:visibility="gone" />

        <View
            android:id="@+id/headline"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/headlist"
            android:background="#ececec" />

        <View
            android:id="@+id/btnheadshow"
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:layout_below="@+id/headline"
            android:background="#00000000" />

        <ImageView
            android:id="@+id/headarray"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:layout_below="@+id/headline"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:src="@drawable/downarray" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/optation"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/head_select_bg">

        <RelativeLayout
            android:id="@+id/optationselect"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_alignParentTop="true">

            <ImageView
                android:id="@+id/optationicon"
                android:layout_width="19dp"
                android:layout_height="19dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="2.5dp"
                android:src="@drawable/powerset" />

            <TextView
                android:id="@+id/optationname"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_marginRight="2.5dp"
                android:layout_toRightOf="@+id/optationicon"
                android:ellipsize="end"
                android:gravity="left|center_vertical"
                android:singleLine="true"
                android:text=""
                android:textColor="#000000"
                android:textSize="10dp" />

        </RelativeLayout>


        <ListView
            android:id="@+id/optationlist"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_below="@+id/optationselect"
            android:visibility="gone" />

        <View
            android:id="@+id/optationline"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/optationlist"
            android:background="#ececec" />

        <View
            android:id="@+id/btnoptationshow"
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:layout_below="@+id/optationline"
            android:background="#00000000" />

        <ImageView
            android:id="@+id/optationarray"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:layout_below="@+id/optationline"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:src="@drawable/downarray" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/headimf"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#ffffff"
        android:visibility="invisible">

        <com.dk.dkphone.view.X5WebView
            android:id="@+id/video"
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_alignParentTop="true"></com.dk.dkphone.view.X5WebView>

        <RelativeLayout
            android:id="@+id/buttom_layer"
            android:layout_width="match_parent"
            android:layout_height="46.5dp"
            android:layout_below="@+id/video"
            android:background="#ffffff">

            <ImageView
                android:id="@+id/youku"
                android:layout_width="140dp"
                android:layout_height="46.5dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="55dp"
                android:src="@drawable/yqtv" />

            <ImageView
                android:id="@+id/tx"
                android:layout_width="140dp"
                android:layout_height="46.5dp"
                android:layout_marginLeft="55dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/youku"
                android:src="@drawable/txtv" />

            <ImageView
                android:id="@+id/aqy"
                android:layout_width="140dp"
                android:layout_height="46.5dp"
                android:layout_marginLeft="55dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/tx"
                android:src="@drawable/aqytv" />

        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/seriesdata"
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="#66000000"
        >


        <TextView
            android:id="@+id/timevalue1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="10dp"
            android:textSize="14dp"
            android:textColor="#f08519"
            android:text="00:00:00"
            />


        <TextView
            android:id="@+id/timevalue2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/timevalue1"
            android:layout_marginTop="10dp"
            android:textSize="9dp"
            android:textColor="#f08519"
            android:text="total:00:00"
            />

        <TextView
            android:id="@+id/speedvalue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="2.5dp"
            android:layout_below="@+id/timevalue2"
            android:layout_marginTop="10dp"
            android:textSize="12.5dp"
            android:textColor="#f08519"
            android:text="100km/h"
            />

        <TextView
            android:id="@+id/selectvalue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="2.5dp"
            android:layout_below="@+id/speedvalue"
            android:layout_marginTop="10dp"
            android:textSize="12.5dp"
            android:textColor="#f08519"
            android:text="100km/h"
            />

        <TextView
            android:id="@+id/hertvalue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="2.5dp"
            android:layout_below="@+id/selectvalue"
            android:layout_marginTop="10dp"
            android:textSize="12.5dp"
            android:textColor="#f08519"
            android:text="100km/h"
            />


        <ImageView
            android:id="@+id/start2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_below="@+id/hertvalue"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/start" />


        <ImageView
            android:id="@+id/add2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/start2"
            android:layout_marginTop="10dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="2.5dp"
            android:src="@drawable/add" />

        <ImageView
            android:id="@+id/des2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/start2"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/add2"
            android:layout_marginLeft="2.5dp"
            android:src="@drawable/des" />

        <ImageView
            android:id="@+id/tv2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/des2"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/tvs" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/shade"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/buttomaciton"
        android:background="#77000000"
        android:visibility="invisible"></RelativeLayout>

    <RelativeLayout
        android:id="@+id/buttomaciton"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_alignParentBottom="true"
        android:background="#000000" />
</RelativeLayout>